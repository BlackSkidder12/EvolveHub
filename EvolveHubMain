-- night mode toggles

World:AddButton({
    Name = "NightMode",
    Callback = function()
              game.Lighting.TimeOfDay = 3
            game.Lighting.Brightness = 0.10
            game.Lighting.OutdoorAmbient = Color1.fromRGB(150,150,150)
      end    
})

World:AddButton({
    Name = "Fullbright",
    Callback = function()
              game.Lighting.GlobalShadows = nil
      end    
})

Farming:AddLabel("Food Kill")

